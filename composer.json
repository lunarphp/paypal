{
  "name": "lunarphp/paypal",
  "type": "project",
  "description": "Paypal payment driver for Lunar.",
  "keywords": ["lunarphp", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop", "cart", "paypal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lunar",
      "homepage": "https://lunarphp.io/"
    }
  ],
  "require": {
    "php": "^8.0",
    "lunarphp/lunar": "^0.7",
    "livewire/livewire": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4.4",
    "orchestra/testbench": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Lunar\\Paypal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lunar\\Paypal\\Tests\\": "tests/"
    }
  },
  "extra": {
    "lunar": {
      "name": "Paypal Payments"
    },
    "laravel": {
      "providers": [
        "Lunar\\Paypal\\PaypalServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
